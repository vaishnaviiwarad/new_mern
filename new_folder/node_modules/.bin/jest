#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/pnpm-store/jest@30.2.0_@types+node@24.10.1/node_modules/jest/bin/node_modules:/mnt/c/pnpm-store/jest@30.2.0_@types+node@24.10.1/node_modules/jest/node_modules:/mnt/c/pnpm-store/jest@30.2.0_@types+node@24.10.1/node_modules:/mnt/c/pnpm-store/node_modules"
else
  export NODE_PATH="/mnt/c/pnpm-store/jest@30.2.0_@types+node@24.10.1/node_modules/jest/bin/node_modules:/mnt/c/pnpm-store/jest@30.2.0_@types+node@24.10.1/node_modules/jest/node_modules:/mnt/c/pnpm-store/jest@30.2.0_@types+node@24.10.1/node_modules:/mnt/c/pnpm-store/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
